<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <ProjectGuid>{01FF183D-C41D-4BB5-B325-F4AE3EFC9F72}</ProjectGuid>
    </PropertyGroup>
    <ItemGroup>
        <Projects Include="photon\zlib\zlib.cbproj">
            <Dependencies/>
        </Projects>
        <Projects Include="photon\libpng\libpng.cbproj">
            <Dependencies/>
        </Projects>
        <Projects Include="photon\tiff-v3.4\tifflib.cbproj">
            <Dependencies/>
        </Projects>
        <Projects Include="photon41.cbproj">
            <Dependencies>photon\zlib\zlib.cbproj;photon\libpng\libpng.cbproj;photon\tiff-v3.4\tifflib.cbproj</Dependencies>
        </Projects>
        <Projects Include="Demo\PhotonDemo.cbproj">
            <Dependencies>photon41.cbproj</Dependencies>
        </Projects>
    </ItemGroup>
    <ProjectExtensions>
        <Borland.Personality>Default.Personality.12</Borland.Personality>
        <Borland.ProjectType/>
        <BorlandProject>
            <Default.Personality/>
        </BorlandProject>
    </ProjectExtensions>
    <Target Name="zlib">
        <MSBuild Projects="photon\zlib\zlib.cbproj"/>
    </Target>
    <Target Name="zlib:Clean">
        <MSBuild Projects="photon\zlib\zlib.cbproj" Targets="Clean"/>
    </Target>
    <Target Name="zlib:Make">
        <MSBuild Projects="photon\zlib\zlib.cbproj" Targets="Make"/>
    </Target>
    <Target Name="libpng">
        <MSBuild Projects="photon\libpng\libpng.cbproj"/>
    </Target>
    <Target Name="libpng:Clean">
        <MSBuild Projects="photon\libpng\libpng.cbproj" Targets="Clean"/>
    </Target>
    <Target Name="libpng:Make">
        <MSBuild Projects="photon\libpng\libpng.cbproj" Targets="Make"/>
    </Target>
    <Target Name="tifflib">
        <MSBuild Projects="photon\tiff-v3.4\tifflib.cbproj"/>
    </Target>
    <Target Name="tifflib:Clean">
